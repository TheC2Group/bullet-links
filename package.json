{
  "name": "c2-bullet-links",
  "version": "1.2.1",
  "description": "This module builds a list of bullet links.",
  "main": "bullet-links.js",
  "scripts": {
    "standalone": "browserify -t [exposify --expose [ --jquery jQuery ] ] bullet-links.js --outfile standalone/bullet-links.js --standalone BulletLinks",
    "dist": "uglifyjs standalone/bullet-links.js --output standalone/bullet-links.min.js --comments",
    "build": "npm run standalone && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TheC2Group/bullet-links.git"
  },
  "keywords": [
    "bullets"
  ],
  "author": "The C2 Group",
  "license": "MIT",
  "devDependencies": {
    "exposify": "^0.5.0"
  },
  "dependencies": {
    "jquery": "^2.1.4"
  }
}
